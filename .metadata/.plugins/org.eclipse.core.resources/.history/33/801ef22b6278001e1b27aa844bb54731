import java.util.Iterator;

public class GuGuDan {

	public static void main(String[] args) {

		System.out.println("구구단");
		System.out.println();

		for (int j = 2; j < 10; j++) {
			System.out.println(j + "단");
			for (int i = 1; i < 10; i++) {
				System.out.println(j + " * " + i + " = " + (j * i));
			}
		}
	}

}
