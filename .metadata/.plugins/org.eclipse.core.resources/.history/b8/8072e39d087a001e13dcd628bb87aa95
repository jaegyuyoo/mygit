import java.util.Scanner;

public class Temp {

	public static void main(String[] args) {
		
	
		int score = 72;
		
		switch (score/10) {
		case 10: 
		case 9: 
			System.out.println("A");
		case 8:
			System.out.println("B");
		case 7:
			System.out.println("C");
		case 6:
		case 5:
		case 4:
		case 3:
		case 2:
		case 1:
			System.out.println("C");
				
		default:
			throw new IllegalArgumentException("Unexpected value: " + score/10);
		}
		
		
/*		
