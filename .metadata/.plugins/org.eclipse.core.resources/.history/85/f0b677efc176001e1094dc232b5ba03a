	public static void main(String[] args) {

			// Scanner 클래서 인스턴스 생성
			Scanner input = new Scanner(System.in);
			
			double amount; // 전체금액(달러)
			int remainingAmount // 센트로 변환
			int numberOfOneDollars; // 달러(100 센터)
			int numberOfQualters; // 쿼터(25 센트)
