import java.util.Iterator;

public class GuGuDan {

	public static void main(String[] args) {

		System.out.println("구구단");
		System.out.println();
		
		for (int i = 2; i < 10; i++) {
			System.out.println(i + "단");
			for (int j = 1l j < 10; j++ ) {
				System.out.println("2 * " + i + " = " + (2 * i));
			}
		
		}
	}

}
